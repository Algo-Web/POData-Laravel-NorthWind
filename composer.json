{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*",
        "cruxinator/class-finder": "1.0.0-RC2|dev-master",
        "algo-web/podata-laravel": "dev-master",
        "mpociot/laravel-test-factory-helper": "^0.4.0",
        "justinrainbow/json-schema": "^5.0@dev",
        "ext-curl": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.2|dev-master",
        "phpunit/phpunit": "~5.6",
        "phpspec/phpspec": "~2.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [

        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jamisonvalenta/Laravel-4-Generators.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/CyberiaResurrection/POData-Laravel.git"
        }
    ]
}
